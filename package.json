    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "3.1.1",
    "sass": "^1.69.5",
    "storybook": "^7.6.4",
    "tailwindcss": "^3.3.6",
